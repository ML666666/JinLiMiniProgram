.listWrapper{
  padding:0rpx 20rpx;
  box-sizing: border-box;
  background: #f5f5f5;
  padding-bottom: 60rpx;
  padding-top: 20rpx;
}
.listWrapper .item{
  /* padding: 44rpx 0; */
  box-sizing: border-box;
}
.listWrapper .item .messageTime{
  text-align: center;
  color: #666;
  margin-bottom:24rpx;
}

.listWrapper .item .time{
  text-align: center;
  color: #666;
  margin-bottom: 14rpx;
  margin-top: 34rpx;
}
.listWrapper .item .container{
  position: relative;
  padding: 32rpx;
  background: #fff;
  box-sizing: border-box;
  border-radius: 10rpx;
}
.listWrapper .item .container .t{
  font-size: 30rpx;
  font-weight: 700;
  margin-bottom: 14rpx;
  margin-top: 14rpx;
  color: #2c3e50;
}
.listWrapper .item .container .c{
  color: #333;
  font-size: 24rpx;
}
.listWrapper .dot{
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background: red;
}