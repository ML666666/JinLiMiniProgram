.LuckyDouble{
  padding: 0 40rpx;
  border-top: 20rpx solid #eee;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  background: #fff;
}
.LuckyDouble .title{
  padding: 20rpx 0;
  border-bottom: 1px solid #f3f3f3;
  font-size: 26rpx;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.LuckyDouble .title text{
  color: red;
}
.LuckyDouble .arrow{
  display: flex;
  align-items: center;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
}
.LuckyDouble .arrow image{
  width: 11rpx;
  height: 20rpx;
  margin-left: 11rpx;
}
.container{
  width: 100%;
  height: 200rpx !important;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  box-sizing: border-box;
}
.container .item{
  align-items: center;
  height: 100rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.container .item .one{
  display: flex;
  align-items: center;
}
.container .item .one .timeWrapper{
  display: flex;
  flex-direction: column;
  margin-left: 15rpx;
}
.container .item .one .timeWrapper view{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
}
.container .item .one .header{
  width: 60rpx;
  height: 60rpx;
  margin-right: 10rpx;
  border-radius: 50%;
}
.container .item .one text{
  color: #2c3e50;
  font-size: 24rpx
}
.container .item .two{
  color: #2c3e50;
  font-size: 24rpx;
  display: flex;
  align-items: center;
}
.container .item .two .w{
  margin-right: 20rpx;
}
.container .btn{
  width:120rpx;
  height:56rpx;
  background:linear-gradient(90deg,rgba(229,41,32,1) 0%,rgba(237,69,60,1) 100%);
  border-radius:8rpx;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  line-height: 56rpx;
}

._CW{
  width: 660rpx;
  height: 880rpx;
  background:rgba(255,255,255,1);
  border-radius:20rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
._CW .titleWrapper{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
  height: 93rpx;
  line-height: 93rpx;
  text-align: center;
  position: relative;
  border-bottom: 1rpx solid rgba(246,246,246,1);
}
._CW .titleWrapper image{
  position: absolute;
  right: 39rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 23rpx;
  height: 23rpx;
}

._CW .wrapper{
  height: 786rpx !important;
  overflow: hidden;
}
._CW .wrapper .item{
  padding:0 20rpx;
  box-sizing: border-box;
  height: 128rpx;
  border-bottom: 1rpx solid rgba(246,246,246,1);
}

.makeSureMask{
  position: absolute;
  width:600rpx;
  height:388rpx;
  background:rgba(255,255,255,1);
  border-radius:20rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.makeSureMask .isToShowMakeSure{
  position: absolute;
  right: 39rpx;
  top: 35rpx;
  width: 23rpx;
  height: 23rpx;
}
.makeSureMask .title{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
  text-align: center;
  line-height: 90rpx;
}
.makeSureMask .timeWrapper{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);
  text-align: center;
}
.imageWrapper{
  width: 232rpx;
  display: flex;
  justify-content: space-between;
  
}
.border{
  border: 4rpx solid #ED453C;
  box-sizing: border-box;
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  position: relative;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.border .user{
  width: 100%;
  height: 100%;
  border-radius: 50%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.border .p{
  width: 62rpx;
  height: 26rpx;
  position: absolute;
  left: -11rpx;
  top: -6rpx;
  z-index: 20;
}
.makeSureMask .btn{
  width:530rpx;
  height:68rpx;
  background:linear-gradient(90deg,rgba(229,41,32,1) 0%,rgba(237,69,60,1) 100%);
  border-radius:8rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  text-align: center;
  line-height: 68rpx;
  color:rgba(255,255,255,1);
}