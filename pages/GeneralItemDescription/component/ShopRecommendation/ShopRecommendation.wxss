.Wrapper{
  padding-top: 30rpx;
  padding-left: 25rpx;
  padding-right: 25rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  background: #fff;
}
.Wrapper .titleWrapper{
  width: 100%;
  display:flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
}
.Wrapper .titleWrapper .title{
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.Wrapper .arrow{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.Wrapper text{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,51,51,1);
  margin-right: 9rpx;
}
.Wrapper image{
  width: 11rpx;
  height: 20rpx;
}
.goodWrapper{
  display: flex;
  justify-content: flex-start;
}
.Wrapper .good{
  width: 224rpx;
}
.Wrapper .good .goodImage{
  width: 224rpx;
  height: 224rpx;
  margin-bottom: 14rpx;
}
.Wrapper .good .title{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(65,65,65,1);
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 20rpx;
}
.priceWrapper{
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.priceWrapper .left{
  font-size:29rpx;
  font-family:PingFang SC;
  font-weight:800;
  color:rgba(255,51,51,1);
}
.priceWrapper .right{
  font-size:18rpx;
  font-family:PingFang SC;
  font-weight:500;
  text-decoration:line-through;
  color:rgba(153,153,153,1);
  text-decoration:line-through;  
}