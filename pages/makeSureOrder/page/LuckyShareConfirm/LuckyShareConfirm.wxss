.LuckyShareConfirmWrapper{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.LuckyShareConfirmWrapper .successIcon{
  width: 100rpx;
  height: 100rpx;
  margin-top: 60rpx;
  margin-bottom: 20rpx;
}
.LuckyShareConfirmWrapper .order-success{
  font-size: 30rpx; 
  color: #ff3339;
  margin-bottom: 60rpx;
}
.LuckyShareConfirmWrapper .btn{
  width: 600rpx;
  margin-bottom: 20rpx;
  height: 102.72rpx;
  padding: 0rpx;
  background: transparent;
  border: none;
}
.LuckyShareConfirmWrapper .btn image{width: 100%;height: 100%;}
.LuckyShareConfirmWrapper .btn:after{
  border:none;
}
.LuckyShareConfirmWrapper .w{
  color: #888;
  margin-bottom: 40rpx;
  font-size: 24rpx;
}
.title_L{
  text-align: center;
  padding: 30rpx 0;
  font-size: 34rpx;
  font-weight: 700;
  color: #2c3e50;
}
.List_L_W{
  margin: 0 30rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding-bottom: 50rpx;
}
.List_L_W .item{
  width: 343.88rpx;
  display: flex;
  box-sizing: border-box;
  background: #fff;
  border-radius: 10rpx;
  padding: 20rpx;
  margin-bottom: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.List_L_W .item image{
  width: 257rpx;
  height: 257rpx;
}
.List_L_W .item view{
  width: 100%;
}
.List_L_W .item .t{
  font-size: 30rpx;
  font-weight: 700;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 10rpx;
}
.List_L_W .item .s{
  color: #888;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 20rpx;
  font-size: 24rpx;
}
.List_L_W .item .p{
  color: #f92d4c;
  font-size: 40rpx;
}