
.card{
  background: #f9f9f9;
}
.card .edit{font-size: 25rpx;color:#fff;}
.card .Wrapper{display: flex;flex-direction: column;align-items: center;justify-content: center;}
.card .Wrapper .NothingWrapper{width: 100%;margin-top:300rpx;display: flex;align-items: center;justify-content: center;flex-direction: column;}
.card .Wrapper .NothingWrapper image{width: 340rpx;height: 160rpx;}
.card .Wrapper .NothingWrapper .tips{font-size:24rpx;font-family:PingFang SC;font-weight:500;color:rgba(102,102,102,1);margin-top: 20rpx;margin-bottom: 30rpx;}
.card .Wrapper .NothingWrapper .btn{
  width:366rpx;
  height:68rpx;
  background:rgba(255,255,255,1);
  border:2rpx solid rgba(255,51,51,1);
  border-radius:8rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,51,51,1);
  line-height: 68rpx;
  text-align: center;
}
.card .Wrapper{
    padding: 20rpx;
    padding-bottom: 150rpx;
}

.card .listWrapper{
  border-radius: 10rpx;
  background: #fff;
}
.card .Wrapper .itemWrapper{
    box-sizing: border-box;
}
.card .Wrapper .itemWrapper .title{
    padding: 26rpx 30rpx;
    border-bottom: 2rpx solid #f2f2f2;
    color: #2c3e50;
    font-size: 24rpx;
    text-align: left;
    width: 100%;
    box-sizing: border-box;
}
.card .Wrapper .itemWrapper .cardList .item{display: flex;justify-content: flex-start;align-items: center;min-height: 200rpx;border-bottom: 2rpx solid #f2f2f2;position: relative;}
.card .Wrapper .itemWrapper .cardList image{width: 150rpx;height: 150rpx;}
.card .Wrapper .itemWrapper .cardList .van-checkbox{margin: 24rpx;}
.card .Wrapper .itemWrapper .item .detail{width: 430rpx;margin-left: 30rpx;}
.card .Wrapper .itemWrapper .t{font-size: 26rpx;color: #555;height: 100rpx;width: 100%;overflow: hidden;}
.card .Wrapper .itemWrapper .s{font-size: 20rpx;color: #c4c4c4;}
.card .Wrapper .itemWrapper .price{height: 50rpx;display: flex;justify-content: space-between;align-items: center;width: 100%;}
.card .Wrapper .itemWrapper .p{font-size: 26rpx;color: #ff1d26;}
.card .Wrapper .itemWrapper .numWrapper{width: 175rpx;display: flex;justify-content: space-around;border: 1rpx solid rgba(119, 119, 119, 0.4)}
.card .Wrapper .itemWrapper .numWrapper view{width: 50rpx;line-height: 50rpx;color: #777;font-size: 36rpx;text-align: center;}
.card .Wrapper .itemWrapper .numWrapper input{width: 75rpx;height: 50rpx; text-align: center;color: #666;border: none;font-size: 26rpx;border-left: 1rpx solid rgba(119, 119, 119, 0.4); border-right: 1rpx solid rgba(119, 119, 119, 0.4)}
.card .buttomCard{background: #fff; width: 100%;height: 100rpx;position: fixed;bottom: 0rpx;left: 0rpx;right: 0rpx;z-index: 50;
display: flex;justify-content: space-between;box-sizing: border-box;align-items: center;}
.card .buttomCard .btn{font-size: 26rpx;color: #f2f2f2;width: 20rpx;-ms-flex-item-align: center;align-self: center;height: 100rpx;text-align: center;background: #fc2d59;line-height: 100rpx;
border: 2rpx solid #ff5f17;width: 200rpx;z-index: 50;}
.card .buttomCard .t{
    font-size: 30rpx;
    margin-left: 40rpx;
}
.card .disable{
    color: #f2f2f2 !important;
    background: #c4c4c4  !important;
    border: 1px solid #c4c4c4  !important;
}
.card .discountAbout{
  display: flex;
  align-items: center;
  font-size: 24rpx;
}

.card .Wrapper .itemWrapper .cardList .deleteItem{
 position: absolute;
 bottom: 0rpx;
 right: 0rpx;
 padding: 25rpx 50rpx;
}
.card .Wrapper .itemWrapper .cardList .deleteItem image{
  width: 40rpx;
  height: 40rpx;
}