.btn{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 160rpx;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  -ms-flex-pack: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 0 26rpx;
  background-color: #d9332e;
}
.btn .b{
  color: #5a381a;
  font-size:32rpx;
  font-weight: 700;
  padding: 30rpx 56rpx;
  line-height: 1;
  border-radius: 80rpx;
  box-sizing: border-box;
  background-color: #f9e592;
}