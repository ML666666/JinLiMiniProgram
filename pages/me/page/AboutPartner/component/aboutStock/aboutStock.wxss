.Low{
  background: #fff;
}
.stock{
  padding-left: 30rpx;
  width: 100%;
  padding-top: 20rpx;
  display: flex;
  box-sizing: border-box;
  flex-direction: column;
}
.title{
  width: 100%;
  display: flex;
}
.stock .item_1{
  width: 418rpx;
  margin-left: 0rpx !important;
}
.stock .item_2{
  width: 100rpx;
}
.stock .item{
  margin-left: 40rpx;
  text-align: left;
}
.stock .title .item{
  font-size:24rpx;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  color:rgba(173,173,173,1);
  text-align: left;
}
.stock .wrapper{
  margin-bottom: 61rpx;
  width: 100%;
  display: flex;
  align-items: center;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.stock .wrapper .item{
  display: flex;
  align-items: center;
}
.stock .wrapper .item .imageHeader{
  width: 100rpx;
  height: 86rpx;
}
.stock .wrapper .item .title{
  font-size:26rpx;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  margin-left: 18rpx;
}
.stock .wrapper .item_2{
  font-size:34rpx;
  font-family:DINPro-Medium,DINPro;
  font-weight:500;
  color:rgba(51,51,51,1);
}