.wrapper{
  background: #fff;
}
.item{
  border-bottom: 10rpx solid #f3f3f3;
}
.item .AgentInfo{
  box-sizing: border-box;
  padding: 20rpx 40rpx;
  display: flex;
  align-items: center;
}
.item .AgentInfo image{
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.item .AgentInfo view{
  font-size: 30rpx;
  color: #2c3e50;
}

.item .AgentPerformance{
  display: flex;
  padding: 0 40rpx;
  border-top: 2rpx solid #f3f3f3;
  box-sizing: border-box;
}
.item .AgentPerformance .en{
  flex: 1;
  padding: 0 40rpx;
  margin: 40rpx 0;
  border-right: 2rpx solid #f3f3f3;
  box-sizing: border-box;
}
.item .AgentPerformance .en .a{
  color: #888;
  margin-bottom: 20rpx;
  text-align: center;
  font-size: 28rpx;
}
.item .AgentPerformance .en .b{
  text-align: center;
  font-size: 28rpx;
  color: #222;
} 