.userDataWrapper{
  background: #fff;
  margin-bottom: 20rpx;
}
.userDataWrapper .income{
  border-bottom: 2px solid #eee;
  padding: 40rpx 0;
  box-sizing: border-box;
  text-align: center;
}
.userDataWrapper .income .MyEarningsTotal{
  color: #f62c3f;
  font-size: 80rpx;
}
.userDataWrapper .income .EarningsText{
  color: #999;
  font-size:30rpx;
}
.performanceeBottom{
  display: flex;
  padding: 30rpx;
}
.performanceeBottom .LastWeek{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.performanceeBottom .LastWeek .num{
  font-size: 36rpx;
  line-height: 30rpx;
  font-weight: 700;
  padding-bottom: 20rpx;
  color: #2c3e50;
}
.performanceeBottom .LastWeek .LastWeekText{
  font-size: 24rpx;
  color: #888;
}