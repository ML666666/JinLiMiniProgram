.itemWrapper{
  background: #fff;
}
.item{
  padding: 0 28rpx;
  border-bottom: 2rpx solid #f3f3f3;
}
.item .MemberInfo{
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  height: 88rpx;
  border-bottom: 2rpx solid #f3f3f3;
}
.item .MemberInfo .MemberPic{
  height: 44rpx;
  width: 44rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 28rpx;
}
.item .MemberInfo .MemberName{
  color: #333;
  font-size: 28rpx;
}
.item .detail{
  margin:28rpx 0;
}
.item .detail .title{
  width: 100%;
  color: #888;
  margin-bottom: 24rpx;
  text-align: center;
  font-size: 28rpx;
}
.item .detail .num{
  font-size: 28rpx;
  color: #222;
  width: 100%;
  text-align: center;
}