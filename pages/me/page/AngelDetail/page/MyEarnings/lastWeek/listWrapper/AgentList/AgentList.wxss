.wrapper{
  background: #fff;
}
.wrapper .AgentBenefitLi{
  border-bottom: 10rpx solid #f3f3f3;
}
.wrapper .AgentBenefitLi .info{
  padding: 20rpx 40rpx;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-pack: justify;
  -ms-flex-align: center;
  align-items: center;
  box-sizing: border-box;
}
.wrapper .AgentBenefitLi .info image{
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.wrapper .AgentBenefitLi .info .name{
  font-size: 30rpx;
  color: #2c3e50;
}
.wrapper .AgentPerformance{
  display: flex;
  padding: 0 40rpx;
  border-top: 2px solid #f3f3f3;
  align-items: center;
  justify-content: space-around;
}
.wrapper .AgentPerformance .item{
  flex: 1;
  padding: 0 40rpx;
  margin: 40rpx 0;
  border-right: 2rpx solid #f3f3f3;
}
.wrapper .AgentPerformance .item .name{
  color: #888;
  margin-bottom: 20rpx;
  font-size: 28rpx;
  text-align: center;
}
.wrapper .AgentPerformance .item .text{
  text-align: center;
  font-size:28rpx;
  color: #222;
}