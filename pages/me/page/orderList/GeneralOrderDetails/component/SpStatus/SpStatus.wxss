.StatueWrapper{
  padding: 20rpx;
  box-sizing: border-box;
  padding-bottom: 60rpx;
}

.status_Wrapper_2{
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  height: 120rpx;
  margin-bottom: 40rpx;
}
.status_Wrapper_2 .btn{
  background: linear-gradient(90deg,#f02b22,#fc2d59);
  height: 76rpx;
  line-height: 76rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  border-radius: 40rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}
.status_Wrapper_2 .e{
  display: flex;
}
.status_Wrapper_2_image_wrapper{
  margin-left: 20rpx;
  display: flex;
}

.status_Wrapper_2_image_wrapper .za{
  position: relative;
}
.status_Wrapper_2_image_wrapper .zw{
  position: absolute;
  top: -15rpx;
  left: -10rpx;
  width: 40rpx;
  height: 40rpx;
}
.status_Wrapper_2_image_wrapper .za_W{
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
  margin-bottom: 4rpx;
  margin-right: 20rpx;
}
.status_Wrapper_2 .tips{
  color: #2c3e50;
  font-size: 24rpx;
}
.status_Wrapper_2 .tips text{
  color: rgb(255, 0, 0);
  margin: 0 5rpx;
}
.status_Wrapper_2 button{
  border: none;
  margin: 0rpx;
}
.status_Wrapper_2_check_btn{
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #f32c32;
  font-size: 24rpx;
  width: 136rpx;
  height: 30rpx;
}
.status_Wrapper_2_check_btn image{
  width: 30rpx;
  height: 30rpx;
}
.status_Wrapper_2 .ws{
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.popupWrapper{
  background: rgb(0, 0, 0,0.5);
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  z-index: 210;
}
.popupWrapper .popup{
  background: #fff;
  border-radius: 30rpx;
  padding-bottom: 60rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 674rpx;
}

.popupWrapper .popup .title{
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 30rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.popupWrapper .popup .title .close{
  position: absolute;
  top: 25rpx;
  right: 20rpx;
  width: 50rpx;
  height: 50rpx;
}
.popupWrapper .popup .item_P{
  display: flex;
  justify-content: space-around;
  font-size: 0;
  margin-bottom: 50rpx;
  height: 100rpx;
  align-items: center;
  width: 100%;
}
.popupWrapper .popup .item_P image{
  border-radius: 50%;
  width: 80rpx;
  height: 80rpx;
}
.popupWrapper .popup .item_P .flex{
  display: flex;
  flex-direction: column;
  -ms-flex-pack: justify;
  justify-content: space-between;
  font-size: 28rpx;
  color: #2c3e50;
  font-size: 28rpx;
}
.popupWrapper .popup .item_P .flex .t{
  color: #f62c3f;
  font-size: 20rpx;
  width: 120rpx;
}
.popupWrapper .popup .sufa{
  padding:0 20rpx;
  font-size: 28rpx;
}
.popupWrapper .popup .sufa .t{
  font-weight: 700;
  color: #2c3e50;
}
.popupWrapper .popup .sufa .red{
  color: red;
}