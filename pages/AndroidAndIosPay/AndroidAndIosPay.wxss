  justify-content: center;
}
.btnWrapper image{
  width: 206rpx;
  height: 200rpx;
  margin-top: 202rpx;
}
.btnWrapper text{
  font-size:36rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
  margin-top: 30rpx;
}
.center{
