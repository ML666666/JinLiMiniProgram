.wrapper{
  padding: 35rpx 25rpx;
  box-sizing: border-box;
}
.userObj{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 36rpx;
}
.userObj .header{
  width: 71rpx;
  height: 71rpx;
  border-radius: 50%;
}
.userObj  .NickName{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
  margin-left: 24rpx;
  margin-right: 28rpx;
}
.userObj .tips{
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);
}

.ProObj{
  display: flex;
  justify-content: space-between;
  margin-top: 32rpx;
}
.ProObj .image{
  width: 176rpx;
  height: 176rpx;
}
.ProObj .detail{
  width: 487rpx;
}
.ProObj .detail .title{
  width: 100%;
  height: 80rpx;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);

  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 36rpx;
}
.priceWrapper{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.priceWrapper .price{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:#FF3333;
}
.priceWrapper .btn{
  width:146rpx;
  height:56rpx;
  background:rgba(255,51,51,1);
  border-radius:6rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  text-align: center;
  line-height: 56rpx;
}
.commend{
  width: 100%;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  /* text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical; */
  margin-top: 40rpx;
}
.imageWrapper{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  padding: 0 15rpx;
  box-sizing: border-box;
}
.imageWrapper image{
  width: 220rpx;
  height: 220rpx;
  margin-bottom: 22rpx;
  margin: 0 10rpx;
}