.LickyDoubleShare{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.LickyDoubleShare .banner{
  width: 600rpx;
  height: 94rpx;
  margin-top: 60rpx;
}
.LickyDoubleShare .user-container{
  padding: 40rpx 112rpx;
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-around;
}
.user-container .header{
  position: relative;
  width: 130rpx;
  height: 130rpx;
  margin-bottom: 43rpx;
  border-radius: 50%;
  border: 2rpx solid #fcd69b;
  box-sizing: content-box;
}
.user-container .pinzu{
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translate(-50%,20%);
  background: #f93;
  width: 68rpx;
  border-radius: 20rpx;
  line-height: 1;
  padding: 4rpx 10rpx;
  text-align: center;
  font-size: 24rpx;
  box-sizing: border-box;
}
.user-container .header image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.user-container .user_Info{
  width: 130rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
}
.user-container .item{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.user-container .p{
  margin-top: 22rpx;
  width: 173rpx;
  height: 108rpx;
}
.Product{
  display: flex;
  justify-content: space-between;
  padding: 0 33rpx 155rpx;
  box-sizing: border-box;
  width: 100%;
}
.Product .ProductImg{
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
  overflow: hidden;
  background: #eee; 
}
.Product .detail{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 390rpx;
  padding: 6rpx 0;
}
.Product .detail .title{
  font-size: 28rpx;
  font-weight: 700;
  line-height: 46rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #2c3e50;
}
.Product .detail .price{
  font-size: 32rpx;
  color: red;
  font-weight: 700;
}
.count-down{
  text-align: center;
  color: #666;
  margin-bottom: 20rpx;
  font-size: 28rpx;
}
.count-down text{
  color: #f33;
}
.btn{
  margin: 0 auto 20rpx;
  line-height: 1;
  padding: 33rpx 0;
  width: 562rpx;
  font-size: 26rpx;
  font-weight: 700;
  background: red;
  text-align: center;
  color: #fff;
  border-radius: 48rpx;
}
.tips{
  text-align: center;
  color: #666;
  font-size: 28rpx;
}
.backHome{
  position: relative;
  width: 100%;
}
.backHome .home{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  left: 35rpx;
  z-index: 210;
  transform: translateY(-50%);
}