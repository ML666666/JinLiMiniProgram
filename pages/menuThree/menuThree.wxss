.OederMask{
  position: fixed;
  top: 0px;
  left: 0px;
  right: 0px;
  bottom: 0px;
  background: rgba(0, 0, 0, 0.6);
  z-index: 300;
  transform: translateY(100%);
  transition: 0.4s;
  transition-timing-function:ease-in-out;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
}
.OederMask .contant .Mask{
  position: absolute;
  left: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  top: 0rpx;
  background: #fff;
  z-index: 100;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  color: #3683d6;
  font-size: 32rpx;
}
.activeMaks{
  transform: translateY(0%);
}


