.deductionTwo{
  width: 100%;
  margin-bottom: 25rpx;
  display: flex;
  flex-direction: column;
  box-shadow:0rpx 0rpx 9rpx 1rpx rgba(0, 0, 0, 0.08);
}
.deductionTwo .banner{
  width: 700rpx; 
}
.goodWrapper{
  padding:0rpx 20rpx;
  box-sizing: border-box;
  flex-direction: column;
  display: flex;
  justify-content: flex-start;
  background: #fff;
}
.goodWrapper .item{
  margin: 25.5rpx 0rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}
.goodWrapper .tag{
  width: 56rpx;
  height: 60rpx;
  position: absolute;
  top: 0rpx;
  right: 21rpx;
}
.goodWrapper .img{
  width: 170rpx;
  height: 170rpx;
  margin-right: 17rpx;
}
.detail{
  height: 170rpx;
}
.detail .t{
  width:363rpx;
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  /* margin-bottom: 29rpx; */
}
.detail .p{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  margin-top: 12rpx;
  margin-bottom: 12rpx;
}
.detail .b{
  /* margin-top: 26rpx; */
  display: flex;
  align-items: center;
}
.detail .daoshou{
  width:80rpx;
  height:28rpx;
  background:rgba(229,41,32,1);
  border:1rpx solid rgba(229,42,33,1);
  border-radius:14rpx;
  font-size:18rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  text-align: center;
  align-items: center;
  margin-right: 11rpx;
}
.detail .b .price{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(229,41,32,1);
}