.MDWrapper{
  width:588rpx;
  height:880rpx;
  background:rgba(255,255,255,1);
  border:1rpx solid rgba(191,191,191,1);
  box-shadow:0px 0px 9rpx 1px rgba(35,48,92,0.17);
  border-radius:20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.MDWrapper .userWapper{
  position: relative;
  width: 250rpx;
  height: 160rpx;
}
.MDWrapper .userWapper .headerWrapper{
  border-radius: 50%;
  width: 130rpx;
  position: absolute;
}
.MDWrapper .userWapper .headerWrapper .text{
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(4,4,4,1);
  text-align: center;
  width: 100%;
  white-space: nowrap;
overflow: hidden;
text-overflow: ellipsis;
}
.MDWrapper .userWapper .headerWrapper .pz{ 
  width: 52rpx;
  height: 23rpx;
  border-radius: 30%;
  position: absolute;
  bottom: 32rpx;
  left: 50%;
  transform: translateX(-50%);
}
.MDWrapper .userWapper .one{
  left: 0rpx;
  top: 0rpx;
  z-index: 10;
}
.MDWrapper .userWapper .two{
  right: 0rpx;
  top: 0rpx;
}
.MDWrapper .userWapper .headerWrapper image{
  width: 100%;
  height: 130rpx;
  border-radius: 50%;
}
.MDWrapper .userWapper .start{
  width: 21rpx;
  height: 21rpx;
  position: absolute;
}
.MDWrapper .userWapper .o{
  top:-10rpx;
  left: -10rpx;
}
.MDWrapper .userWapper .t{
  left: -50rpx;
  top: 90rpx;
}
.MDWrapper .userWapper .f{
  top: 60rpx;
  right: -40rpx;
}
.MDWrapper .banner{
  width: 341rpx;
  height: 124rpx;
  margin-bottom: 55rpx;
}

.MDWrapper .tips{
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 300rpx;
}

.MDWrapper .tips .center{
  font-size:36rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(4,4,4,1);
}

.MDWrapper .tips .line{
  width:32rpx;
  height:3rpx;
  background:rgba(0,0,0,1); 
}
.MDWrapper .tips_2{
  height:43rpx;
  font-size:45rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(255,69,94,1);
  margin-top: 24rpx;
  margin-bottom: 24rpx;
}

.MDWrapper .tips_3{
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(169,167,167,1);
}
.MDWrapper .good_Item{
  display: flex;
  width: 490rpx;
  justify-content: space-between;
  align-items: center;
  margin-top: 22rpx;
}
.MDWrapper .good_Item image{
  width: 152rpx;
  height: 126rpx;
  margin-right: 22rpx;
}
.good_Item .title{
  font-size:22rpx;
  font-family:MicrosoftYaHei;
  font-weight:400;
  color:rgba(4,4,4,1);
  line-height:34rpx;
}
.good_Item .e .i{
  font-size:34rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(255,75,99,1);
  margin-right: 22rpx;
}
.good_Item .e .w{
  font-size:22rpx;  
  font-family:PingFang-SC-Medium;
  font-weight:500;
  text-decoration:line-through;
  color:rgba(187,187,187,1);
}

.btn{
  width: 490rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  margin-top: 20rpx;
  background: linear-gradient(to right, #FE4A48 , #FC383C);
  border-radius: 60rpx;
  font-size: 36rpx;
  color: #fff;
  font-weight: 500;
}