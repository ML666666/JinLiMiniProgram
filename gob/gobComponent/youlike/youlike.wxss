.youlike{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding-bottom: 100rpx;
}
.youlike .top{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  margin-top: 43rpx;
  margin-bottom: 34rpx;
}
.youlike .top image{
  width: 27rpx;
  height: 25rpx;
}
.youlike .top .t{
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
  margin: 0 15rpx;
}
.goodList{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding-left: 25rpx;
  padding-right: 24rpx;
  box-sizing: border-box
}
.goodList .good{
  width: 340rpx;
  margin-bottom: 20rpx;
}
.goodList .good .imgaeBanner{
  width: 340rpx;
  height: 342rpx;
}
.goodList .detail{
  padding: 25rpx;
  box-sizing: border-box;
}
.goodList .detail .title{
  height: 66rpx;
  width: 100%;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,1);
  /* margin-bottom: 10rpx; */
}
.goodList .detail .sell{
  font-size:18rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
  color: #999999;
}
.priceWrapper{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.priceWrapper .price{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:800;
  color:rgba(230,47,38,1);
  margin-right: 33rpx;
}
.priceWrapper ._price{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:800;
  text-decoration:line-through;
  color:rgba(170,170,170,1);
}